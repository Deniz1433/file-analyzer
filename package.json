{
  "name": "file-analyzer-electron",
  "version": "1.0.0",
  "description": "An Electron app for file analysis",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
